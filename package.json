{
  "name": "drive_peek",
  "private": true,
  "type": "module",
  "scripts": {
    "build:css:compile": "sass ./app/assets/stylesheets/application.bootstrap.scss:./app/assets/builds/application.css --no-source-map --load-path=node_modules",
    "build:css:prefix": "postcss ./app/assets/builds/application.css --use autoprefixer -o ./app/assets/builds/application.css",
    "build:css": "yarn run build:css:compile && yarn run build:css:prefix"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@hotwired/turbo-rails": "^8.0.20",
    "@popperjs/core": "^2.11.8",
    "@rails/ujs": "^7.1.3-4",
    "autoprefixer": "^10.4.22",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "jquery": "^3.7.1",
    "nodemon": "^3.1.11",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "sass": "^1.94.2"
  },
  "browserslist": [
    "defaults"
  ]
}
